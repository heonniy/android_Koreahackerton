        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_main1"
        app:srcCompat="@drawable/search_compoment_box" />

    <TextView
        android:id="@+id/title_box1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/aninormal"
        android:text="나에게 맞는 평생 교육 TOP 3"
        android:textSize="16sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.106"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <TextView
        android:id="@+id/title_sub_box1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/aninormal"
        android:text="AI (GPT) 가 당신을 위해, 분석한 결과는 . . . "
        android:textSize="13sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.168"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/title_box1" />

    <TextView
        android:id="@+id/result_text_top1"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/aninormal"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toStartOf="@+id/imageView4" />

    <TextView
        android:id="@+id/result_text_top2"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/aninormal"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/result_text_top1" />

    <TextView
        android:id="@+id/result_text_top3"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/aninormal"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/result_text_top2" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="70dp"
        android:src="@drawable/search_main_box_scroll_back_rec"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/search_final_box3" />

    <TextView
        android:id="@+id/result_text_inbox_top1"
        android:layout_width="265dp"
        android:layout_height="21dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="70dp"
        android:fontFamily="@font/gmarketsansttfbold"
        android:letterSpacing="0.09"
        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10" />

    <TextView
        android:id="@+id/result_text_inbox_top2"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="280dp"
        android:fontFamily="@font/gmarketsansttfbold"
        android:letterSpacing="0.09"

        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10" />

    <TextView
        android:id="@+id/result_text_inbox_top3"
        android:layout_width="90dp"
        android:layout_height="25dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="500dp"
        android:fontFamily="@font/gmarketsansttfbold"
        android:letterSpacing="0.09"
        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10" />

    <TextView
        android:id="@+id/result_text_inbox_top1_explain"
        android:layout_width="269dp"
        android:layout_height="118dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/aninormal"
        android:letterSpacing="0.09"
        android:lineSpacingExtra="4dp"
        android:text="잠시만 기다려주세요"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toBottomOf="@+id/result_text_inbox_top1" />

    <TextView
        android:id="@+id/result_text_inbox_top2_explain"
        android:layout_width="269dp"
        android:layout_height="118dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/aninormal"
        android:letterSpacing="0.09"
        android:lineSpacingExtra="4dp"
        android:text="잠시만 기다려주세요"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toBottomOf="@+id/result_text_inbox_top2" />

    <TextView
        android:id="@+id/result_text_inbox_top3_explain"
        android:layout_width="269dp"
        android:layout_height="118dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="525dp"
        android:fontFamily="@font/aninormal"
        android:letterSpacing="0.09"
        android:lineSpacingExtra="4dp"
        android:text="잠시만 기다려주세요"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10" />

    <WebView
        android:id="@+id/HomewebView"
        android:layout_width="0dp"
        android:layout_height="1dp"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/result_text_inbox_top3_explain"
    app:layout_constraintVertical_bias="0.0"
    tools:ignore="WebViewLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>