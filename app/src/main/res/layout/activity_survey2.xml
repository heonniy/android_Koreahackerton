<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".survey_2">


    <TextView
        android:id="@+id/address3"
        android:layout_width="221dp"
        android:layout_height="25dp"

        android:layout_marginBottom="28dp"
        android:fontFamily="@font/anilight"
        android:maxLines="1"
        android:text="©CppVa 2023 Korea hackerton"
        android:textSize="12sp"

        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/total_2"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/survey_2_img"
        android:layout_width="168dp"
        android:layout_height="148dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.928"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.115"
        app:srcCompat="@drawable/yeppi" />

    <ImageView
        android:id="@+id/total_2"
        android:layout_width="336dp"
        android:layout_height="456dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address"
        app:layout_constraintVertical_bias="0.012"
        app:srcCompat="@drawable/total_2" />

    <ImageButton
        android:id="@+id/book_button"
        android:layout_width="57dp"
        android:layout_height="59dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="@+id/total_2"
        app:layout_constraintTop_toTopOf="@+id/total_2"
        app:layout_constraintVertical_bias="0.197"
        app:srcCompat="@drawable/book"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/readingbook_button"
        android:layout_width="70dp"
        android:layout_height="58dp"
        android:layout_marginBottom="322dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.257"
        app:layout_constraintStart_toEndOf="@+id/book_button"
        app:layout_constraintTop_toTopOf="@+id/total_2"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/readingbook"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/smart_learn_button"
        android:layout_width="59dp"
        android:layout_height="56dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintEnd_toEndOf="@+id/total_2"
        app:layout_constraintHorizontal_bias="0.612"
        app:layout_constraintStart_toEndOf="@+id/readingbook_button"
        app:layout_constraintTop_toTopOf="@+id/total_2"
        app:layout_constraintVertical_bias="0.198"
        app:srcCompat="@drawable/smart_learn"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/sports_button"
        android:layout_width="68dp"
        android:layout_height="56dp"
        android:layout_marginStart="23dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintStart_toStartOf="@+id/total_2"
        app:layout_constraintTop_toBottomOf="@+id/book_button"
        app:layout_constraintVertical_bias="0.224"
        app:srcCompat="@drawable/sports"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/painting_button"
        android:layout_width="62dp"
        android:layout_height="60dp"
        android:layout_marginStart="48dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintStart_toEndOf="@+id/sports_button"
        app:layout_constraintTop_toBottomOf="@+id/readingbook_button"
        app:layout_constraintVertical_bias="0.217"
        app:srcCompat="@drawable/painting"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/music_button"
        android:layout_width="77dp"
        android:layout_height="64dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintEnd_toEndOf="@+id/total_2"
        app:layout_constraintHorizontal_bias="0.655"
        app:layout_constraintStart_toEndOf="@+id/painting_button"
        app:layout_constraintTop_toBottomOf="@+id/smart_learn_button"
        app:layout_constraintVertical_bias="0.214"
        app:srcCompat="@drawable/music"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/job_button"
        android:layout_width="67dp"
        android:layout_height="61dp"
        android:layout_marginStart="24dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintStart_toStartOf="@+id/total_2"
        app:layout_constraintTop_toBottomOf="@+id/sports_button"
        app:layout_constraintVertical_bias="0.44"
        app:srcCompat="@drawable/job"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/cooking_button"
        android:layout_width="66dp"
        android:layout_height="59dp"
        android:layout_marginStart="44dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintStart_toEndOf="@+id/job_button"
        app:layout_constraintTop_toBottomOf="@+id/painting_button"
        app:layout_constraintVertical_bias="0.438"
        app:srcCompat="@drawable/cooking"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/chess_button"
        android:layout_width="80dp"
        android:layout_height="63dp"
        android:background="@color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="@+id/total_2"
        app:layout_constraintEnd_toEndOf="@+id/total_2"
        app:layout_constraintHorizontal_bias="0.69"
        app:layout_constraintStart_toEndOf="@+id/cooking_button"
        app:layout_constraintTop_toBottomOf="@+id/music_button"

        app:layout_constraintVertical_bias="0.41"
        app:srcCompat="@drawable/chess"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="40dp"
        android:fontFamily="@font/anibold"
        android:maxLines="1"
        android:text="관심분야 선택"
        android:textSize="23sp"
        android:textStyle="normal"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.227" />


</androidx.constraintlayout.widget.ConstraintLayout>