<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.MainMenuMypageFragment">

    <TextView
        android:id="@+id/address5"
        android:layout_width="221dp"
        android:layout_height="25dp"

        android:layout_marginTop="4dp"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/anilight"
        android:maxLines="1"
        android:text="©CppVa 2023 Korea hackerton"

        android:textSize="12sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.589"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="368dp"
        android:layout_height="161dp"
        android:contentDescription="TODO"
        android:src="@drawable/change_mypage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ContentDescription,HardcodedText" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:layout_marginTop="76dp"
        android:src="@drawable/mypage_compo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView13" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="290dp"
        android:layout_height="400dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/mypage_rec_ec"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView13"
        app:layout_constraintVertical_bias="0.055" />

    <EditText
        android:id="@+id/username"
        android:layout_width="120dp"
        android:layout_height="28dp"
        android:layout_marginTop="48dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/aninormal"
        android:gravity="center_horizontal"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:letterSpacing="0.09"
        android:text="김감자"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        tools:ignore="TouchTargetSizeCheck" />


    <EditText
        android:id="@+id/nickname"
        android:layout_width="120dp"
        android:layout_height="28dp"
        android:layout_marginTop="101dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/aninormal"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:text="감자돌이님"
        android:textSize="13sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.305"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/id_1"
        android:layout_width="wrap_content"
        android:layout_height="28dp"
        android:layout_marginTop="338dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/aninormal"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:text="cppva@naver.com"
        android:textSize="13sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.38"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView13"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/tb1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="10dp"
        android:layout_marginTop="108dp"
        android:fontFamily="@font/aninormal"
        android:gravity="center_vertical"
        android:text="별명 :"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/nickname"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <TextView
        android:id="@+id/tb3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="10dp"
        android:layout_marginTop="38dp"
        android:fontFamily="@font/aninormal"
        android:gravity="center_vertical"
        android:text="아이디 :"
        android:textStyle=""
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/id_1"
        app:layout_constraintHorizontal_bias="0.636"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/tb1" />

    <TextView
        android:id="@+id/tb4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="20dp"
        android:layout_marginTop="92dp"
        android:fontFamily="@font/aninormal"
        android:gravity="center_vertical"
        android:text="비밀번호 :"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/tb1" />

    <TextView
        android:id="@+id/tb6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="20dp"
        android:layout_marginTop="145dp"
        android:fontFamily="@font/aninormal"
        android:gravity="center_vertical"
        android:text="전화번호 :"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/tb1" />

    <TextView
        android:id="@+id/tb7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="20dp"
        android:layout_marginTop="198dp"
        android:fontFamily="@font/aninormal"
        android:gravity="center_vertical"
        android:text="주소 :"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/tb1" />

    <TextView
        android:id="@+id/password_1"
        android:layout_width="43dp"
        android:layout_height="18dp"

        android:layout_marginStart="76dp"
        android:layout_marginTop="92dp"
        android:fontFamily="@font/aninormal"
        android:gravity="center_vertical"
        android:text="*******"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/tb1" />

    <TextView
        android:id="@+id/tb2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="108dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/aninormal"
        android:gravity="center_vertical"
        android:text="나이 :"
        android:textSize="13sp"
        app:layout_constraintEnd_toStartOf="@+id/age"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <EditText
        android:id="@+id/phone_number_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="268dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/aninormal"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:text="010-1234-1234"
        android:textSize="13sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/address_1"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="322dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/aninormal"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:text="울산군 울주읍"
        android:textSize="13sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.184"
        app:layout_constraintStart_toEndOf="@+id/tb7"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="108dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/aninormal"
        android:gravity="right"
        android:imeOptions="actionDone"
        android:inputType="text"
        android:text="40"
        android:textSize="13sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.729"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        tools:ignore="TouchTargetSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>